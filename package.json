{
  "name": "@larbish/supabase",
  "version": "0.0.24",
  "description": "Supabase module for Nuxt3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larbish/supabase-module.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "prepack": "yarn build",
    "release": "yarn lint && standard-version && git push --follow-tags"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "@supabase/supabase-js": "^1.31.0",
    "defu": "^5.0.1",
    "pathe": "^0.2.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.1.7",
    "@nuxtjs/eslint-config-typescript": "^9.0.0",
    "eslint": "^8.10.0",
    "nuxt3": "latest",
    "standard-version": "^9.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/nuxt-community/supabase-module/issues"
  },
  "homepage": "https://github.com/nuxt-community/supabase-module#readme",
  "directories": {
    "example": "example"
  }
}
